{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "bot",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js"
  ],
  "license": "MIT",
  "author": "unrealg",
  "type": "module",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "prod": "ts-node --transpile-only ./src/main.ts",
    "serve": "node build/main.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/utilities": "^3.1.5",
    "discord.js": "^13.3.1",
    "discordx": "^9.0.1",
    "dotenv": "^16.1.4",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.19",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
