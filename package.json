{
  "name": "discordbot",
  "version": "0.0.1",
  "description": "DiscordJS in Typescript!",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/client.js"
  },
  "keywords": [
    "discord.js",
    "typescript",
    "command"
  ],
  "author": "unrealg",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@types/node": "^17.0.2",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0"
  }
}
